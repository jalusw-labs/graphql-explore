{
  "name": "graphql-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "start": "node ./dist/index.js",
    "start:watch": "nodemon ./dist/index.js",
    "dev": "ts-node ./src/index.ts",
    "format": "prettier --write ./src",
    "lint": "eslint ./src",
    "test": "jest",
    "test:watch": "jest --watch ./dist",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "type": "commonjs",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "graphql": "^16.9.0",
    "mongodb": "^6.10.0",
    "mongoose": "^8.7.3"
  },
  "license": "MIT"
}
